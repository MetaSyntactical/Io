{
    "name": "metasyntactical/io",
    "type": "library",
    "description": "The MetaSyntactical InputOutput component",
    "keywords": ["library", "component", "file", "io"],
    "homepage": "http://syntactical-sugar.com/projects/io",
    "license": "MIT",
    "authors": [
        {
            "name": "Daniel Kreuer",
            "email": "dk@metasyntactical.com"
        },
        {
            "name": "MetaSyntactical Community",
            "homepage": "http://syntactical-sugar.com/contributors"
        }
    ],
    "require": {
        "php": "8.0.*"
    },
    "require-dev": {
        "infection/infection": "^0.24.0",
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "MetaSyntactical\\Io\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MetaSyntactical\\Io\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "preferred-install": {
            "*": "dist"
        }
    }
}
